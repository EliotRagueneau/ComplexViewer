            "name": "Colin Combe",
            "email": "colin.combe@ed.ac.uk"
        },
        {
            "name": "Josh Heimbach",
            "email": "joshkh@gmail.com"
        },
        {
            "name": "Yo Yehudi",
            "email": "yochannah@gmail.com"
        },
        {
            "name": "Marine Dumousseau",
            "email": ""
        }
    ],
    "license": "Apache-2.0",
    "devDependencies": {
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "babel-loader": "^8.1.0",
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "eslint": "^7.2.0"
    },
    "scripts": {
        "clean": "rm dist/complexviewer.js",
        "build-dev": "webpack -d --mode development --config webpack.conf.js",
        "build-prod": "webpack -p --mode production --config webpack.conf.js"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "browserify-shim": {
        "./bower_components/colorbrewer/colorbrewer.js": "colorbrewer"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:MICommunity/interaction-viewer.git"
    },
    "dependencies": {
        "colorbrewer": "^1.3.0",
        "d3": "~3.5.5",
        "intersectionjs": "^1.0.1",
        "point2d": "^0.0.1",
        "rgbcolor": "^1.0.0",
        "spin.js": "^4.1.0",
        "webcola": "^3.4.0"
    },
    "keywords": [
        "biojs",
        "protein",
        "interactions",
        "complexes"
    ],
    "main": "src/controller/Controller.js"
}
